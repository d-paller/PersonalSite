@using System.Web.Configuration
@using System.Web.Optimization

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>

    @* Does this use the CDNs??? *@
    @Styles.Render("~/Content/bootstrap.css", "~/Content/bootstrap-theme.css", "~/Content/Style.css")
    @Scripts.Render("~/Scripts/Tracking.js") @* Tracking must begin at the beginning of the page load. *@
    

</head>

<body>

    @Html.Partial("Navbar")

    @RenderBody()

    @Html.Partial("Footer")


    @* Scripts and Styles --------------------------------- *@

    @*<script src="~/scripts/jquery-3.1.0.js"></script>*@
    @Scripts.Render("~/bundles/jquery")

    <script type="text/javascript">
        //load jquery if CDN fails
        if (typeof jquery == undefined) {
            var e = document.createElement('script');
            e.src = '@Url.Content("~/Scripts/jquery-{version}.js")';
            e.type = 'text/javascript';
            document.getElementsByTagName("head")[0].appendChild(e);
        }
    </script>

    @*<script src="~/Scripts/bootstrap.min.js"></script>*@
    @Scripts.Render("~/bundles/bootstrap")

    @*<script src="~/Scripts/modernizr-2.6.2.js"></script>*@
    @Scripts.Render("~/bundles/modernizer")

    @*<script src="~/scripts/CustomScript.js"></script>*@
    @Scripts.Render("~/bundles/javascript")


    @RenderSection("scripts", required: false)

</body>


</html>